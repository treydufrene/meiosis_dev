function [X] = FK(gamma)
    T = zeros(3,3,7);
    T(:,:,1) = rotz(gamma(1));
    T(:,:,2) = T(:,:,1)*rotz(gamma(2))*roty(pi/2);
    T(:,:,3) = T(:,:,2)*rotz(gamma(3));
    T(:,:,4) = T(:,:,3)*rotz(gamma(4))*rotx(-pi/2);
    T(:,:,5) = T(:,:,4)*rotz(gamma(5))*rotx(pi/2);
    T(:,:,6) = T(:,:,5)*rotz(gamma(6))*rotx(-pi/2);
    
    jointr(:,1) = [0;       0;    8];
    jointr(:,2) = [0;    -1.54;  14.08];
    jointr(:,3) = [0;       25;      0];
    jointr(:,4) = [0;       20;      0];
    jointr(:,5) = [0;       0;       8];
    jointr(:,6) = [0;       0;       0];
    jointr(:,7) = [0;       0;    5.25];

    r = zeros(3,7);
    r(:,1) = jointr(:,1);
    for i = 2:7
        r(:,i) = r(:,i-1) + T(:,:,i-1)*jointr(:,i);
    end
    
    q_0 = sqrt(trace(T(:,:,7) + 1))/2;
    if(q_0 ~= 0)
        q_1 = (T(3,2,7) - T(2,3,7))/(4*q_0);
        q_2 = (T(1,3,7) - T(3,1,7))/(4*q_0);
        q_3 = (T(2,1,7) - T(1,2,7))/(4*q_0);
    end
    if(q_0 == 0)
        q_1 = sqrt((T(1,1,7) + 1)/2);
        q_2 = sqrt((T(2,2,7) + 1)/2);
        q_3 = sqrt((T(3,3,7) + 1)/2);
    end
    
    pos = r(:,7);
    ori = [q_1; q_2; q_3];
    X = [ori;pos];
end